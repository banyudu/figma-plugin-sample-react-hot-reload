{
  "name": "figma-plugin-sample-react-hot-reload",
  "version": "1.0.0",
  "description": "Your Figma plugin",
  "main": "code.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev:plugin": "cross-env NODE_ENV=development webpack --mode=development --watch",
    "dev:ui": "cross-env NODE_ENV=development webpack serve --mode=development",
    "dev": "run-p dev:plugin dev:ui",
    "dev:mock": "cross-env MOCK=true npm run dev",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode=production",
    "analyze": "cross-env NODE_ENV=production ANALYZE=true webpack --mode=production",
    "lint": "eslint . --fix",
    "pretty": "prettier --write './**/*.(js|ts|jsx|tsx|md|html)'",
    "pack": "rimraf figma-plugin.zip && npm run build && cd dist && bestzip ../figma-plugin.zip *",
    "prepare": "husky"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@eslint/js": "~9.39.1",
    "@figma/plugin-typings": "^1.103.0",
    "@types/lodash": "^4.17.12",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bestzip": "^2.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.1",
    "prettier": "^3.3.3",
    "react-dev-utils": "^12.0.1",
    "react-refresh": "^0.14.2",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "typescript": "^5.7.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.96.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "antd": "^5.22.2",
    "axios": "^1.7.9",
    "eventemitter3": "^5.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "uuid": "^11.0.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,less,md}": "prettier --write"
  }
}
